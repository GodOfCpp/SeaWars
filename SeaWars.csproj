<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>battleship-logo.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="battleship-logo.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="battleship-logo.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="battleship-logo.ico" />
  </ItemGroup>

</Project>